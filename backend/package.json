{
  "name": "imagine-this-printed-backend",
  "version": "1.0.0",
  "description": "Express TypeScript backend for Imagine This Printed",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "prestart": "node ./scripts/check-jose.js",
    "start": "node dist/index.js",
    "dev": "tsx --env-file=.env index.ts",
    "watch": "tsx watch --env-file=.env index.ts",
    "worker": "tsx --env-file=.env worker/index.ts",
    "worker:dev": "tsx watch --env-file=.env worker/index.ts",
    "dev:full": "concurrently \"npm:watch\" \"npm:worker:dev\" --names \"API,Worker\" --prefix-colors \"blue,magenta\"",
    "generate:stock-models": "tsx --env-file=.env scripts/generate-stock-models.ts",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:logs": "pm2 logs"
  },
  "keywords": [
    "express",
    "typescript",
    "api",
    "prisma"
  ],
  "author": "Imagine This Printed",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/storage": "^7.17.3",
    "@prisma/client": "^5.22.0",
    "@supabase/supabase-js": "^2.77.0",
    "@types/uuid": "^10.0.0",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jose": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^5.1.6",
    "node-fetch": "^2.7.0",
    "openai": "^6.8.0",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.2",
    "replicate": "^1.3.1",
    "serpapi": "^2.2.1",
    "stripe": "^17.4.0",
    "tsx": "^4.20.6",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.19.0",
    "@types/node-fetch": "^2.6.13",
    "concurrently": "^9.2.1",
    "prisma": "^5.22.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
